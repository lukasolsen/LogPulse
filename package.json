{
  "name": "@lukasolsen/logpulse",
  "version": "1.0.1",
  "description": "A simple logging library",
  "main": "logpulse/logpulse.js",
  "types": "logpulse/logpulse.d.ts",
  "type": "commonjs",
  "scripts": {
    "test": "node ./test.js",
    "build": "tsc",
    "build-npm": "tsc",
    "build-website": "webpack --mode production",
    "dev-website": "webpack serve --mode development",
    "dev": "nodemon --watch src/**/*.ts --exec ts-node ./src/logpulse.ts",
    "start": "ts-node ./src/loglogpulseify.ts"
  },
  "author": "Lukas Olsen",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "auto-changelog": "^2.4.0",
    "babel-loader": "^9.1.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "nodemon": "^3.0.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@huggingface/inference": "^2.6.1"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "remote": "LogPulse",
    "handlebarsSetup": "templates/setup.js",
    "issueUrl": "https://www.github.com/lukasolsen/logpulse/issues/{id}",
    "issuePattern": "[A-Z]+-\\d+",
    "unreleased": true
  }
}
